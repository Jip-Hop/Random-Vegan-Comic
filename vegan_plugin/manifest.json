{
  "name": "Random Vegan Comic",
  "description": "Occasionally show a random vegansidekick.com image",
  "version": "1.0",
  "icons": { "16": "icon16.png",
            "128": "icon128.png" },  
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
