{
  "name": "Random Vegan Comic",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Occasionally show a random vegansidekick.com image",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["random_vegan_script.js"]
    }
  ]
}
